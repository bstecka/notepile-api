{
  "manifest_version": 2,
  "name": "NotePile",
  "version": "0.1",
  "permissions": ["storage"],
  "icons": { "16": "icon16.png"},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js","content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {}
}